{
  "name": "steds-gallery",
  "version": "1.0.43",
  "main": "index.js",
  "license": "MIT",
  "nodemonConfig": {
    "verbose": true,
    "ext": [
      "js",
      "mjs"
    ],
    "ignore": [
      "server/data/*.json"
    ],
    "delay": 2500
  },
  "scripts": {
    "reset": "rm gallery.sqlite&&yarn prisma migrate dev --name init",
    "seedA": "clear&&node prisma/seedA.mjs ",
    "seedP": "clear&&node prisma/seedP.mjs ",
    "moveFiles": "clear&&node prisma/moveFiles.mjs --trace-warnings ",
    "generate": "npx prisma generate",
    "server": "clear&&pnpm nodemon -r dotenv/config ./server/index.js",
    "phpServer": "php -S 127.0.0.1:8000",
    "setPrerelease": " pnpm version prerelease -f ",
    "deploy": "clear&& pnpm version patch && node -r dotenv/config ./deploy.mjs",
    "uploadGalleryDB": "clear&& node -r dotenv/config ./uploadGalleryDB.mjs",
    "downloadGalleryDB": "clear&& node -r dotenv/config ./downloadGalleryDB.mjs"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/multipart": "^7.7.0",
    "@fastify/static": "^6.10.2",
    "basic-ftp": "^5.0.3",
    "colors": "^1.4.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.18.0",
    "fastify-file-upload": "^4.0.0",
    "form-data": "^4.0.0",
    "fs-jetpack": "^5.1.0",
    "getenv": "^1.0.0",
    "jimp": "^0.22.8",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "log-update": "^5.0.1",
    "mailgun.js": "^9.1.1",
    "mysql2": "^3.4.0",
    "node-fetch": "2",
    "ospoint": "^0.2.1",
    "pino": "^8.14.1",
    "prettier": "^2.8.8",
    "sequelize": "^6.32.1",
    "textmagic-rest-client": "^1.0.11",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.1.2",
    "@types/node": "^20.3.1",
    "cloudinary": "^1.40.0",
    "csv": "^6.3.1",
    "csv-parse": "^5.4.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "volta": {
    "node": "20.12.2"
  }
}
